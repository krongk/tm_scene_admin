<%= simple_form_for(@template) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :cate %>
    <%= f.input :title %>
    <%= f.input :price %>
    <%= f.input :init_section_keys %>
    <%= f.input :html %>
    <%= f.input :form %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
