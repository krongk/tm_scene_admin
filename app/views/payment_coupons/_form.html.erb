<%= simple_form_for(@payment_coupon) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :code %>
    <%= f.input :price %>
    <%= f.input :message %>
    <%= f.input :status %>
    <%= f.input :actived_by %>
    <%= f.input :actived_site_id %>
    <%= f.input :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
