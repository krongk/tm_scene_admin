<h1>Listing payment_coupons</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Code</th>
      <th>Price</th>
      <th>Message</th>
      <th>Status</th>
      <th>Actived by</th>
      <th>Actived site</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @payment_coupons.each do |payment_coupon| %>
      <tr>
        <td><%= payment_coupon.user %></td>
        <td><%= payment_coupon.code %></td>
        <td><%= payment_coupon.price %></td>
        <td><%= payment_coupon.message %></td>
        <td><%= payment_coupon.status %></td>
        <td><%= payment_coupon.actived_by %></td>
        <td><%= payment_coupon.actived_site_id %></td>
        <td><%= payment_coupon.note %></td>
        <td><%= link_to 'Show', payment_coupon %></td>
        <td><%= link_to 'Edit', edit_payment_coupon_path(payment_coupon) %></td>
        <td><%= link_to 'Destroy', payment_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Payment coupon', new_payment_coupon_path %>
