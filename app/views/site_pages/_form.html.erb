<%= simple_form_for(@site_page) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :site %>
    <%= f.input :key %>
    <%= f.input :title %>
    <%= f.input :keywords %>
    <%= f.input :description %>
    <%= f.input :content %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
